{
    "name": "GW2WAPIF",
    "description": "Autofill API field on GW2 wiki pages.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "*://wiki.guildwars2.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "img/icon16.png",
            "32": "img/icon32.png",
            "48": "img/icon48.png",
            "64": "img/icon64.png",
            "128": "img/icon128.png"
        }
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png",
        "128": "img/icon128.png"
    }
}